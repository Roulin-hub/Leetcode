#include <stdbool.h>
#include <string.h>
bool isValid(char* s){
int n=strlen(s);
char stack[n];
int top=-1;
for(int i=0;i<n;i++){
char ch=s[i];
if(ch=='('||ch=='{'||ch=='[')
stack[++top]=ch;
else{
if(top==-1)
return false;
char t=stack[top--];
if((ch==')'&&t!='(')||(ch=='}'&&t!='{')||(ch==']'&&t!='['))
return false;
}
}
return top==-1;
}
